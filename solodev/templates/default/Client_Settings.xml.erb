<?phpxml version="1.0" encoding="UTF-8"?>
<settings>
	<setting name="client">
		<name>Client Name</name>
		<shortcut>%client%</shortcut>
		<value><%= node[:install][:client_name] %></value>
		<default></default>
	</setting>
	<setting name="website">
		<name>Website Address</name>
		<shortcut>%website%</shortcut>
		<value></value>
		<default></default>
	</setting>
	<setting name="logo">
		<name>Logo</name>
		<shortcut>%logo%</shortcut>
		<value></value>
		<default></default>
	</setting>
	<setting name="email">
		<name>Support Email</name>
		<shortcut>%email%</shortcut>
		<value></value>
		<default></default>
	</setting>
  <setting name="client-root">
		<name>Client Root</name>
		<shortcut>%cr%</shortcut>
		<value>Main</value>
		<default></default>
	</setting>
	<setting name="aws">
		<name>AWS Settings</name>
		<shortcut>%aws%</shortcut>
		<value>1</value>
		<default></default>
	</setting>
	<setting name="aws-key">
		<name>AWS Key</name>
		<shortcut>%awskey%</shortcut>
		<value></value>
		<default></default>
	</setting>
	<setting name="aws-secret">
		<name>AWS Secret</name>
		<shortcut>%awssecret%</shortcut>
		<value></value>
		<default></default>
	</setting>
	<setting name="aws-cdn">
		<name>AWS CDN</name>
		<shortcut>%awscdn%</shortcut>
		<value></value>
		<default></default>
	</setting>
	<setting name="stml-cache">
		<name>STML Cache</name>
		<shortcut>%stmlcache%</shortcut>
		<value>86400</value>
		<default></default>
	</setting>
	<setting name="file-cache">
		<name>File Cache</name>
		<shortcut>%filecache%</shortcut>
		<value>86400</value>
		<default></default>
	</setting>
	<setting name="dbms">
		<name>DBMS Name</name>
		<shortcut>%dbms%</shortcut>
		<value>MySQL</value>
		<default></default>
	</setting>
	<setting name="mongo">
		<name>Mongo</name>
		<shortcut>%mongo%</shortcut>
		<value>1</value>
		<default></default>
	</setting>
	<setting name="mongo-hostname">
		<name>Mongo</name>
		<shortcut>%mongo%</shortcut>
		<value><%= @private_ip %></value>
		<default></default>
	</setting>
	<setting name="db-database">
		<name>Database Name</name>
		<shortcut>%dbn%</shortcut>
		<value><%= node[:install][:DBName] %></value>
		<default></default>
	</setting>
	<setting name="db-hostname">
		<name>DB Server Hostname</name>
		<shortcut>%dbhn%</shortcut>
		<value><%= node[:install][:DBHOST] %></value>
		<default></default>
	</setting>
	<setting name="db-write-hostname">
		<name>DB Server Hostname</name>
		<shortcut>%dbhn%</shortcut>
		<value><%= node[:install][:DBHOST] %></value>
		<default></default>
	</setting>
	<setting name="db-username">
		<name>DBMS Username</name>
		<shortcut>%dbun%</shortcut>
		<value><%= node[:install][:DBUSER] %></value>
		<default></default>
	</setting>
	<setting name="db-password">
		<name>DBMS Password</name>
		<shortcut>%dbpw%</shortcut>
		<value><%= node[:install][:DBPASSWORD] %></value>
		<default></default>
	</setting>
	<setting name="db-write-password">
		<name>DBMS Password</name>
		<shortcut>%dbpw%</shortcut>
		<value><%= node[:install][:DBPASSWORD] %></value>
		<default></default>
	</setting>
	<setting name="ODBC">
		<name>ODBC</name>
		<shortcut>%odbc%</shortcut>
		<value>0</value>
		<default>1</default>
	</setting>
	<setting name="response-from-name">
		<name>response-from-name</name>
		<shortcut>%rfn%</shortcut>
		<value></value>
		<default></default>
	</setting>
	<setting name="response-from-email">
		<name>response-from-email</name>
		<shortcut>%rfe%</shortcut>
		<value></value>
		<default></default>
	</setting>
	<setting name="completed-status">
		<name>completed-status</name>
		<shortcut>%acs%</shortcut>
		<value>Completed</value>
		<default></default>
	</setting>
	<setting name="approved-status">
		<name>approved-status</name>
		<shortcut>%acs%</shortcut>
		<value>Approved</value>
		<default></default>
	</setting>
	<setting name="inprogress-status">
		<name>inprogress-status</name>
		<shortcut>%aps%</shortcut>
		<value>In Progress</value>
		<default></default>
	</setting>
	<setting name="notstarted-status">
		<name>notstarted-status</name>
		<shortcut>%aps%</shortcut>
		<value>Not Started</value>
		<default></default>
	</setting>
	<setting name="display-overall-notstarted-status">
		<name>display-overall-notstarted-status</name>
		<shortcut>%aps%</shortcut>
		<value>0</value>
		<default></default>
	</setting>
	<setting name="denied-status">
		<name>denied-status</name>
		<shortcut>%aps%</shortcut>
		<value>Denied</value>
		<default></default>
	</setting>
	<setting name="datatable_overview_tab">
		<name>overview</name>
		<order>1</order>
		<shortcut>%aps%</shortcut>
		<value>Request Overview</value>
		<default></default>
	</setting>
	<setting name="datatable_feedback_tab">
		<name>feedback</name>
		<order>2</order>
		<shortcut>%aps%</shortcut>
		<value>Current Status</value>
		<default></default>
	</setting>
	<setting name="default-response-view">
		<name>default-response-view</name>
		<shortcut>%aps%</shortcut>
		<value>showResponsesLinear.tpl</value>
		<default>showResponses.tpl</default>
	</setting>
	<setting name="outbound-link-tracking">
		<name>outbound-link-tracking</name>
		<shortcut>%olt%</shortcut>
		<value>recordOutboundLink(this,'Outbound_Links','url_replace');</value>
		<default></default>
	</setting>
	<setting name="document-link-tracking">
		<name>document-link-tracking</name>
		<shortcut>%dlt%</shortcut>
		<value>documentTrack('url_replace');</value>
		<default></default>
	</setting>
	<setting name="SSLCertificateLocation">
		<name>SSLCertificateLocation</name>
		<shortcut>%sslcert%</shortcut>
		<value></value>
		<default></default>
	</setting>
	<setting name="captcha-background">
		<name>captcha-background</name>
		<shortcut>%cb%</shortcut>
		<value>ffffff</value>
		<default></default>
	</setting>
	<setting name="captcha-noise">
		<name>captcha-noise</name>
		<shortcut>%cn%</shortcut>
		<value>cccccc</value>
		<default></default>
	</setting>
	<setting name="captcha-text">
		<name>captcha-text</name>
		<shortcut>%ct%</shortcut>
		<value>000000</value>
		<default></default>
	</setting>
	<setting name="use-css">
		<name>use-css</name>
		<shortcut>%us%</shortcut>
		<value>1</value>
		<default></default>
	</setting>
</settings>